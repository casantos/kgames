#define DoNormalLib YES
#define DoSharedLib YES
#define DoDebugLib YES
#define LibName Xkw
#define SoRev SOXKWREV
#define HasSharedData YES
#define IncSubdir X11
#define IncSubSubdir Xkw

SOXKWREV=1.0

INCLUDES=-I.. -D_DEFAULT_SOURCE  -Wall -Wextra -Wmissing-prototypes -Wstrict-prototypes
        HEADERS = Cards.h CardsP.h CardsUtil.h SuitCards.h Hand.h HandP.h \
		  Thermo.h ThermoP.h Layout.h LayoutP.h Pad.h PadP.h
           SRCS = Cards.c Hand.c Thermo.c Layout.c laygram.c laylex.c \
	   	  CardsUtil.c SuitCards.c Animate.c Message.c Pad.c medium.c
           OBJS = Cards.o Hand.o Thermo.o Layout.o laygram.o laylex.o \
	   	  CardsUtil.o SuitCards.o Animate.o Message.o Pad.o medium.o
  CARDS_BITMAPS = playing_card bob spade_lg

#include <Library.tmpl>

depend :: laygram.c laylex.c 

DependTarget()

laygram.c : laygram.y
	yacc -d laygram.y
	sed 's/yy/LayYY/g' y.tab.c > laygram.c
	sed 's/yy/LayYY/g' y.tab.h > laygram.h
	rm y.tab.c y.tab.h

laygram.h : laygram.c

clean::
	-rm laygram.c laygram.h

laylex.c: laylex.l
	lex laylex.l
	sed 's/yy/LayYY/g' lex.yy.c > laylex.c
	rm lex.yy.c

clean::
	-rm laylex.c
